{
  "presets": ["next/babel"],
  "plugins": [
    "@emotion",
    "@babel/plugin-proposal-optional-chaining",
    [
      "module-resolver",
      {
        "alias": {
          "public": "./public"
        },
        "root": ["./src/"],
        "extensions": [".ts", ".tsx", ".js", ".jsx"]
      }
    ]
  ]
}
